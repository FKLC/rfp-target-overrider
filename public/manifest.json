{
  "manifest_version": 2,
  "name": "RFPTarget Overrider",
  "version": "1.0.0",
  "browser_action": {
    "default_title": "RFPTarget Overrider",
    "default_popup": "index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "overridehelper@example.com"
    }
  },
  "experiment_apis": {
    "fppOverrides": {
      "schema": "schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "paths": [["fppOverrides"]],
        "script": "api.mjs"
      }
    }
  }
}
